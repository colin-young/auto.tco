        $(document).ready(function () {
            $("#slider-thumbnail").owlCarousel({
                items: 3,
                autoPlay : true,
                stopOnHover : false,
                items:4,
                itemsDesktop: [1199,4],
                itemsDesktopSmall: [979,3],
                itemsTablet: [600,2],
                itemsMobile: [479,1],
                navigation: true,
                navigationText:['<i class="fa fa-angle-left"></i>','<i class="fa fa-angle-right"></i>']
            });
        });
    </script>
}

@section TopbarSlider {
